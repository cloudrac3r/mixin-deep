{
  "name": "@cloudrac3r/mixin-deep",
  "description": "Deeply mix the properties of objects into the first object. Like merge-deep, but doesn't clone. No dependencies.",
  "version": "3.0.1",
  "homepage": "https://github.com/cloudrac3r/mixin-deep",
  "author": "Cadence Ember (https://github.com/cloudrac3r)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudrac3r/mixin-deep.git"
  },
  "bugs": {
    "url": "https://github.com/cloudrac3r/mixin-deep/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "mocha"
  },
  "devDependencies": {
    "gulp-format-md": "^2.0.0",
    "mocha": "^6.1.4"
  },
  "private": false,
  "keywords": [
    "assign",
    "deep",
    "extend",
    "key",
    "keys",
    "merge",
    "mixin",
    "object",
    "prop",
    "properties",
    "util",
    "values"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "defaults-deep",
        "extend-shallow",
        "merge-deep",
        "mixin-object"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
